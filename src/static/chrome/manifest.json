{
  "name": "StackGuru Integration",
  "version": "{{version}}",
  "update_url": "{{update_url}}",
  "description": "Integrates StackGuru into the Stackoverflow site.",
  "icons": { "128": "icon128.png", "48": "icon48.png", "32": "icon32.png", "16": "icon16.png" },
  "permissions": [
    "{{base_url}}"
  ],
  "content_scripts": [
    { "matches": ["*://*.stackoverflow.com/questions/*", "*://*.stackexchange.com/questions/*"], "js": ["jquery.min.js", "followLink.js"] }
  ]
}
